{
  "version": 2,
  "builds": [
    {
      "src": "manage.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb" }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "manage.py"
    }
  ],
    "rewrites": [
    { "source": "/(.*)", "destination": "/SintoniaMental/wsgi.py" }
  ]
  }